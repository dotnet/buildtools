<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ProducesOutputDir Condition="'$(ProducesOutputDir)' == ''">$(BinDir)produces/</ProducesOutputDir>
    <ProducesPublishFolder Condition="'$(ProducesPublishFolder)' == ''">$(ProducesOutputDir)artifacts/</ProducesPublishFolder>
    <ProducesJsonFilename Condition="'$(ProducesJsonFilename)' == ''">$(ProducesOutputDir)produces.json</ProducesJsonFilename>
    <VersionsRepoPath Condition="'$(VersionsRepoPath)' == '' and '$(ProducesRepo)' != '' and '$(ProducesBranch)' != ''">build-info/dotnet/$(ProducesRepo)/$(ProducesBranch)</VersionsRepoPath>
  </PropertyGroup>
</Project>